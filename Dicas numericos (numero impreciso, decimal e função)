console.log(7 / 0) // vai aparecer "Infinity" pois é uma operação de numero infinito
console.log("10" / 2) // em outras linguagens o "10" não daria certo por ser string, mais no javascript da certo
console.log("3" + 2) // usando o + aqui ele vai concatenar pois string tem preferencia
console.log("Show!" / 2) // não é um numero
console.log(0.1 + 0.7) // não vai dar 0.8 pois o javascript não tem suporte pra numero flutuante vai dar 0.79999 (IMPRECISO)
// console.log(10.ToString()) esse aqui não da certo pois o 10 não esta entre ""
console.log((10.345).toFixed(2)) // para mostrar duas casas decimais apenas (PARA POR FUNÇÂO COLOQUE O NUMERO ENTRE ()
