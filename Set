const { time } = require("console")

//não aceita repetição e não é indexado (não tem um indice)
const times = new Set()
times.add('Palmeiras')
times.add('Corinthias').add('Santos').add('Juventos').add('Barcelona')
times.add('Palmeiras') //como o palmeiras ja existe, esse vai ser ignorado !!
console.log(times)

console.log(times.size) //para saber quantos times tem
console.log(times.has('Palmeiras')) //para saber se tem o palmeiras
times.delete('Corinthias')

