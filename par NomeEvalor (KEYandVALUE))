// par nome/valor (key/value)

const saudacao = 'opa' //contexto lexico 1

function exec() {
    const saudacao = 'falaa' // contexto lexico 2
    return saudacao
}
console.log(saudacao)
console.log(exec())

// objetos s√£o grupos aninhados de pares nome/valor(key/value)]
